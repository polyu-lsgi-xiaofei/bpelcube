        <wsdl:part element="tns:version_parameter" name="version"/>
        <wsdl:part element="tns:procedure_parameter" name="procedure"/>
        <wsdl:part element="tns:outputFormat_parameter" name="outputFormat"/>
    </wsdl:message>
    <wsdl:message name="DescribeSensorResponse">
        <wsdl:part element="sml:SensorML" name="response"/>
    </wsdl:message>
    <wsdl:message name="GetObservationRequest">
        <wsdl:part element="tns:request_parameter_GetCapabilities" name="request"/>
        <wsdl:part element="tns:service_parameter" name="service"/>
        <wsdl:part element="tns:version_parameter" name="version"/>
        <wsdl:part element="tns:responseFormat_parameter" name="responseFormat"/>
        <wsdl:part element="tns:offering_parameter" name="offering"/>
        <wsdl:part element="tns:observedProperty_parameter" name="observedProperty"/>
    </wsdl:message>
    <wsdl:message name="GetObservationResponse">
        <wsdl:part element="om:Observation" name="response"/>
    </wsdl:message>
    <wsdl:message name="ServiceException">
        <wsdl:part element="ows:ExceptionReport" name="exception"/>
    </wsdl:message>
    <wsdl:portType name="Precipitation_PortType" sawsdl:modelReference="http://giv-wfs.uni-muenster.de/jcr/repository/y2review/SOS10_BrgmSos/Precipitation.rdf#SOS10_BrgmSos:Precipitation">
