            <wsdl:input message="tns:GetCapabilitiesRequest" name="GetCapabilitiesRequest"/>
            <wsdl:output message="tns:GetCapabilitiesResponse" name="GetCapabilitiesResponse"/>
            <wsdl:fault message="tns:ServiceException" name="ExceptionReport"/>
        </wsdl:operation>
        <wsdl:operation name="DescribeSensor">
            <wsdl:input message="tns:DescribeSensorRequest" name="DescribeSensorRequest"/>
            <wsdl:output message="tns:DescribeSensorResponse" name="DescribeSensorResponse"/>
            <wsdl:fault message="tns:ServiceException" name="ExceptionReport"/>
        </wsdl:operation>
        <wsdl:operation name="GetObservation">
            <wsdl:input message="tns:GetObservationRequest" name="GetObservationRequest"/>
            <wsdl:output message="tns:GetObservationResponse" name="GetObservationResponse"/>
            <wsdl:fault message="tns:ServiceException" name="ExceptionReport"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="SOS10_BrgmSos_HttpGET" type="tns:Precipitation_PortType">
        <http:binding verb="GET"/>
        <wsdl:operation name="GetCapabilities">
            <http:operation location="constellation-envision/WS/sos-discovery?"/>
            <wsdl:input name="GetCapabilitiesRequest">
                <http:urlEncoded/>
            </wsdl:input>
            <wsdl:output name="GetCapabilitiesResponse">
                <mime:content part="response" type="text/xml"/>
            </wsdl:output>
            <wsdl:fault name="ExceptionReport"/>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:binding name="SOS10_BrgmSos_HttpPOST" type="tns:Precipitation_PortType">
        <http:binding verb="POST"/>
        <wsdl:operation name="GetCapabilities">
            <http:operation location="constellation-envision/WS/sos-discovery?"/>
            <wsdl:input name="GetCapabilitiesRequest">
                <http:urlEncoded/>
            </wsdl:input>
            <wsdl:output name="GetCapabilitiesResponse">
                <mime:content part="response" type="text/xml"/>
            </wsdl:output>
            <wsdl:fault name="ExceptionReport"/>
