        <wsdl:port binding="tns:SOS10_BrgmSos_HttpGET" name="SOS10_BrgmSos_HttpGETPort">
            <http:address location="http://swe.brgm.fr/"/>
        </wsdl:port>
        <wsdl:port binding="tns:SOS10_BrgmSos_HttpPOST" name="SOS10_BrgmSos_HttpPOSTPort">
            <http:address location="http://swe.brgm.fr/"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>