            <xsd:element name="outputFormat_parameter" type="xsd:string"/>
            <xsd:element name="responseFormat_parameter" type="xsd:string"/>
            <xsd:element name="offering_parameter" type="xsd:anyURI"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="GetCapabilitiesRequest">
        <wsdl:part element="tns:request_parameter_GetCapabilities" name="request"/>
        <wsdl:part element="tns:service_parameter" name="service"/>
        <wsdl:part element="tns:version_parameter" name="version"/>
    </wsdl:message>
    <wsdl:message name="GetCapabilitiesResponse">
        <wsdl:part element="sos:Capabilities" name="response"/>
    </wsdl:message>
    <wsdl:message name="DescribeSensorRequest">
        <wsdl:part element="tns:request_parameter_GetCapabilities" name="request"/>
